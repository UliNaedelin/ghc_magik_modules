def_slotted_exemplar(:ghc_copy_from_wo_to_nrm,
        {
		{:wo_dr_view, _unset},
		{:wo_ws_view, _unset},
		{:wo_ts_view, _unset},	
		{:nrm_ww_view, _unset},
		{:nrm_gw_view, _unset},
