			    p_nrm_co_view)
_endmethod
$

_pragma(classify_level=restricted)
_method ghc_copy_from_wo_to_nrm.init(p_wo_dr_view,
				     p_nrm_ww_view,
				     p_wo_ts_view,
				     p_nrm_ts_view,
				     p_wo_ws_view,
