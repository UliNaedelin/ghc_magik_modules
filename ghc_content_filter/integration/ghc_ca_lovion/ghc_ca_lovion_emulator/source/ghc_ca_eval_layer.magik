_pragma(classify_level=restricted)
_method ghc_ca_eval_layer.new(p_name,
				p_repository)
	##
	##
	_return _clone.init(p_name,
			    p_repository)
_endmethod
$


_pragma(classify_level=restricted)
_method ghc_ca_eval_layer.init(p_name,
				 p_repository)
	## 
	## 
	.name << p_name
	.repository << p_repository
	p_repository.layers.add(_self)
	_return _self 
_endmethod
